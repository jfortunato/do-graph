{
  "name": "do-graph",
  "version": "1.0.0",
  "main": "src/index.js",
  "author": "developer@jfortunato.com",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^4.4.2"
  },
  "devDependencies": {
    "esbuild": "^0.20.2",
    "typescript": "^5.4.4"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "yarn typecheck && esbuild src/index.ts src/index.html --bundle --minify --sourcemap --loader:.html=copy --outdir=dist",
    "watch": "esbuild src/index.ts src/index.html --bundle --sourcemap --loader:.html=copy --outdir=dist --watch",
    "clean": "rm -rf dist"
  }
}
